/* pages/myinfo/reportpage/reportpage.wxss */

.back-view{
  display: flex;
  flex-direction: column;
}

.gridview{

  margin: 5rpx 10rpx 5rpx 10rpx;
}

.gridview-item{
  margin: 1%;
  width: 48%;
  height: 100rpx;
  float: left;
  background: white
}

.gridview-cell{
  padding-left: 30rpx;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  font-size: 30rpx;
  color: #292929;
}

.empty-view{
  height: 100%;
  width: 20rpx;
}

.title-view{
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 15rpx;
  font-size: 30rpx;
  color: #333;
}

.push-button{
  height: 100rpx;
  margin-right: 30rpx;
  margin-left: 30rpx;
  margin-top: 40rpx;
  background-color: #ffa500;
  border-radius: 10rpx;
  color: white;
  font-size: 36rpx;
  text-align: center;
  line-height: 100rpx;
}