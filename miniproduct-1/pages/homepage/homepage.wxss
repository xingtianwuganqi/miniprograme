/* pages/homepage/homepage.wxss */

page{
  background-color: #f5f5f5;
}

.scroll-page{
  background-color: white;
}

.scroll-cell{
  background-color: #eeeeee;
  display: flex;
  flex-direction: column;
}

.userInfo{
  background-color: white;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  width: 100%;
}

.head-img{
  width: 60rpx;
  height: 60rpx;
  border-radius: 30rpx;
  vertical-align: center;
}

.userName{
  background-color: white;
  display: flex;
  flex-direction: column;
  color: #333333;
  font-size: 32rpx;
}

.time-item{
  font-size: 24rpx;
  color: #999999;
}